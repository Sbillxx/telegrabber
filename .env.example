# Telegram API Credentials
# Dapatkan dari: https://my.telegram.org/apps
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Session String (opsional, akan dibuat otomatis saat pertama kali login)
# Setelah login pertama kali, copy session string dari console dan simpan di sini
SESSION_STRING=

# Server Port (opsional, default: 3000)
PORT=3000

# Untuk login pertama kali (opsional, bisa diisi manual saat diminta)
PHONE_NUMBER=+6281234567890
PASSWORD=your_2fa_password_if_any
OTP_CODE=12345

